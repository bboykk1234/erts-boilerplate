{
  "name": "erts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build:dev:main": "cross-env NODE_ENV=development webpack --color --progress --config ./configs/webpack.config.main.ts",
    "build:prod:main": "cross-env NODE_ENV=production webpack --color --progress --config ./configs/webpack.config.main.ts",
    "build:dev:renderer": "cross-env NODE_ENV=development webpack --color --progress --config ./configs/webpack.config.renderer.ts",
    "build:prod:renderer": "cross-env NODE_ENV=production webpack --color --progress --config ./configs/webpack.config.renderer.ts",
    "server": "webpack-dev-server --color --progress --config ./configs/webpack.config.renderer.ts",
    "build:prod": "npm run build:prod:renderer && npm run build:main",
    "start": "npm run build && npm run electron",
    "start:dev": "npm run build:dev:main && npm run server",
    "electron": "electron ./app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bboykk1234/erts-boilerplate.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "sqlite"
  ],
  "author": "David Tan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bboykk1234/erts-boilerplate/issues"
  },
  "homepage": "https://github.com/bboykk1234/erts-boilerplate#readme",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/webpack": "^4.39.1",
    "@types/webpack-dev-server": "^3.1.7",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.7",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "sqlite3": "^4.1.0"
  }
}
